{
  "name": "@procgenie/shared-types",
  "version": "1.0.0",
  "private": true,
  "description": "Shared TypeScript types for the ProcGenie S2P Orchestration Platform",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./intake": {
      "types": "./src/intake.ts",
      "default": "./src/intake.ts"
    },
    "./workflow": {
      "types": "./src/workflow.ts",
      "default": "./src/workflow.ts"
    },
    "./buying": {
      "types": "./src/buying.ts",
      "default": "./src/buying.ts"
    },
    "./agent": {
      "types": "./src/agent.ts",
      "default": "./src/agent.ts"
    },
    "./supplier": {
      "types": "./src/supplier.ts",
      "default": "./src/supplier.ts"
    },
    "./contract": {
      "types": "./src/contract.ts",
      "default": "./src/contract.ts"
    },
    "./analytics": {
      "types": "./src/analytics.ts",
      "default": "./src/analytics.ts"
    },
    "./sustainability": {
      "types": "./src/sustainability.ts",
      "default": "./src/sustainability.ts"
    },
    "./integration": {
      "types": "./src/integration.ts",
      "default": "./src/integration.ts"
    },
    "./security": {
      "types": "./src/security.ts",
      "default": "./src/security.ts"
    },
    "./common": {
      "types": "./src/common.ts",
      "default": "./src/common.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
